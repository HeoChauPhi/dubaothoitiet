<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="shortcut icon" href="../images/favicon.ico">
    <title>{{ site_title }} - {% block title %}{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% block head %}
      <link rel="stylesheet" href="../css/styles.css">
    {% endblock %}
    <script src="../js/libs/ouical-custom.js"></script>
  </head>
  <body>
    {% block header %}
      {% include 'header.twig' %}
      <h1>{% block title %}{% endblock %}</h1>
    {% endblock %}
    {% block content %}{% endblock %}

    {% block footer %}
      {% include 'footer.twig' %}
    {% endblock %}
    {% block footer_scripts %}
      <script src="script/jquery.min.js"></script>
      <script src="../js/load-youtube.js"></script>
      <script src="../js/libs/media.match.min.js"></script>
      <script src="../js/libs/enquire.min.js"></script>
      <script src="../js/libs/modernizr.min.js"></script>
      <script src="../js/libs/chosen.jquery.min.js"></script>
      <script src="../js/libs/slick.js"></script>
      <script src="../js/libs/jquery.matchHeight-min.js"></script>
      <script src="../js/libs/jquery.fancybox.pack.js"></script>
      <script src="../js/libs/stacktable.min.js"></script>
      <script src="../js/script.js"></script>
      <script src="https://www.youtube.com/iframe_api"></script>
    {% endblock %}
  </body>
</html>
