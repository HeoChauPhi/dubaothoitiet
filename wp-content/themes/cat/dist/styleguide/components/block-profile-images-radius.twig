
{% set profile_content = [
    {
      'profile_content_image' : '../images/images-demo/profile1-radius.png',
      'profile_content_title' : 'Rovert Andersson',
      'profile_content_position' : 'Chief Executive Offcer (CEO)',
      'profile_content_email' : 'robert@catenamedia.com',
      'profile_content_phone' : '+35677032928'
    }
  ]
%}

<div class="block-profile block-profile--image-radius">
  <div class="container-wide">
    <div class="block-profile__list">
      {% for item in profile_content %}
        <div class="block-profile__item">
          <div class="block-profile__image"><img src="{{ item.profile_content_image}}"></div>
          <div class="block-profile-image__group">
            <h3  class="block-profile__title">{{ item.profile_content_title }}</h3>
            <div class="block-profile__position">{{ item.profile_content_position }}</div>
            <div class="block-profile__email">{{ item.profile_content_email }}</div>
            <div class="block-profile__phonenumber">{{ item.profile_content_phone }}</div>
          </div>
        </div>
      {% endfor %}
    </div>
  </div>
</div>
